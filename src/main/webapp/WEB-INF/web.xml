<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0">
  
  <servlet>
     <servlet-name>Login</servlet-name>
     <servlet-class>propertycontrol.Loginservlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name> Login</servlet-name>
     <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>landlordinsert</servlet-name>
     <servlet-class>propertycontrol.LandlordInsertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name> landlordinsert</servlet-name>
     <url-pattern>/insert</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
      <servlet-name>updatelandlord</servlet-name>
      <servlet-class>propertycontrol.UpdateprofileServlet</servlet-class>
  
  </servlet>
  
  <servlet-mapping>
      <servlet-name>updatelandlord</servlet-name>
      <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>deletelandlord</servlet-name>
      <servlet-class>propertycontrol.DeleteLandlordServlet</servlet-class>
  
  </servlet>
  <servlet-mapping>
      <servlet-name>deletelandlord</servlet-name>
      <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>




